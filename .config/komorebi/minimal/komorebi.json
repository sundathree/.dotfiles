{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.36/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/.config/komorebi/applications.json",
  "window_hiding_behaviour": "Cloak",
  "mouse_follows_focus": false,
  "default_workspace_padding": 4,
  "default_container_padding": 4,
  "border_style": "System",
  "border": false,
  "border_width": 2,
  "border_offset": -1,
  "theme": {
    "palette": "Base16",
    "name": "DefaultDark",
    "unfocused_border": "Base00",
    "single_border": "Base07",
    "floating_border": "Base00"
  },
  "ignore_rules": [
    {
      "id": "osu!.exe",
      "kind": "Exe"
    },
    {
